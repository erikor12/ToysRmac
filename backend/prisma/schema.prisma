generator client {
  provider = "prisma-client-js"
  // default output (node_modules/.prisma/client) â€” do not set custom output when using @prisma/client
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model products {
  ID        Int     @id @default(autoincrement())
  STORE     String
  NAME      String?
  YEAR      Int?
  VALUE     Int?
  DESC      String?
}
